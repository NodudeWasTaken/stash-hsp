{
  "name": "stash-hsp",
  "type": "module",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@types/bun": "latest",
    "graphql": "^16.9.0",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "bun run ./src/index.ts",
    "bundle": "bun build ./src/index.ts --minify --sourcemap --target bun --outdir ./build",
    "compile": "bun build ./src/index.ts --compile --minify --sourcemap --target bun --outfile stashhsp",
    "dev": "DEBUG=express:* bun --watch run ./src/index.ts",
    "prettier": "prettier ./src --write",
    "prettier-check": "prettier ./src --check",
    "generate-types": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.10.8",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "p-limit": "^6.1.0",
    "request-ip": "^3.3.0",
    "sharp": "^0.33.4"
  }
}